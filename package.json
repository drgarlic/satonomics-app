{
  "name": "satonomics",
  "description": "Satoshi Economics",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "($npm_execpath outdated || read -p \"Press enter to ignore...\") && vite --host",
    "build": "vite build",
    "check": "tsc --noEmit --skipLibCheck --pretty",
    "check-watch": "$npm_execpath check --watch",
    "format": "prettier --write './src'",
    "prod": "$npm_execpath run build && vite preview --host",
    "serve": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "license": "MIT",
  "dependencies": {
    "@leeoniya/ufuzzy": "^1.0.14",
    "@solid-primitives/event-listener": "^2.3.3",
    "@solid-primitives/intersection-observer": "^2.1.6",
    "@solid-primitives/media": "^2.2.8",
    "@solid-primitives/memo": "^1.3.8",
    "@solid-primitives/mouse": "^2.0.19",
    "@solid-primitives/resize-observer": "^2.0.25",
    "@solid-primitives/scheduled": "^1.4.3",
    "@solid-primitives/timer": "^1.3.9",
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.13.1",
    "fancy-canvas": "^2.1.0",
    "lean-qr": "^2.3.4",
    "lightweight-charts": "^4.1.3",
    "solid-js": "^1.8.15"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@iconify-json/tabler": "^1.1.109",
    "@tailwindcss/container-queries": "^0.1.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.37",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "vite": "^5.1.6",
    "vite-plugin-pwa": "^0.19.5",
    "vite-plugin-solid": "^2.10.2",
    "workbox-window": "^7.0.0"
  }
}
